  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/mocha": "8.2.2",
    "@types/node": "15.3.0",
    "mocha": "8.4.0",
    "source-map-support": "0.5.19",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
