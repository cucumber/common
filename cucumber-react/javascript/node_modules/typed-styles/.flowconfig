[ignore]
<PROJECT_ROOT>/lib/
\..*\.js
.*\.config\.js
.*\.json

[options]
suppress_type=$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIgnore
suppress_comment=\\(.\\|\n\\)*\\$ExpectError

[lints]
all=warn
sketchy-null-bool=off
sketchy-null-string=off
sketchy-null-number=off
