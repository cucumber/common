  },
  "homepage": "https://github.com/cucumber/gherkin-javascript",
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.9",
    "mocha": "^6.0.2",
    "nyc": "^14.1.0",
    "prettier": "^1.17.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "c21e": "cucumber/c21e-javascript",
    "cucumber-messages": "cucumber/cucumber-messages-javascript"
