{
  "name": "cucumber-html-formatter",
  "version": "1.1.0",
  "description": "HTML formatter for Cucumber",
  "bin": {
    "cucumber-html-formatter": "bin/cucumber-html-formatter.js"
  },
  "main": "dist/src/cli-main.js",
  "types": "dist/src/cli-main.d.ts",
  "repository": "https://github.com/cucumber/cucumber",
  "author": "Aslak Helles√∏y",
  "license": "MIT",
  "scripts": {
    "nyc": "nyc --reporter=html --reporter=text mocha",
    "test": "mocha",
    "build": "tsc && webpack-cli src/main.tsx",
    "pretty-quick": "pretty-quick --staged"
  },
  "dependencies": {
    "cucumber-messages": "^6.0.1",
    "cucumber-react": "^1.1.0",
    "jsdom": "^15.1.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@types/jsdom": "^12.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.11",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "mocha": "^6.2.1",
    "npm-link-shared": "^0.5.6",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  }
}
