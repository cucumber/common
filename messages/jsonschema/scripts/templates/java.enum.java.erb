<% @enums.each do |enum| -%>
    public enum <%= enum[:name] %> {
    <% enum[:values].each_with_index do |value, index| -%>
    <%= enum_constant(value) %>("<%= value %>")<%= index < enum[:values].length-1 ? ',' : ';' %>
    <% end -%>

        private final String value;

        <%= enum[:name] %>(String value) {
            this.value = value;
        }

        @Override
        public String toString() {
            return this.value;
        }

        public String value() {
            return this.value;
        }

        public static <%= enum[:name] %> fromValue(String value) {
        <%- enum[:values].each do |value| -%>
            if ("<%= enum_constant(value) %>".equals(value)) return <%= enum_constant(value) %>;
        <%- end -%>
            throw new IllegalArgumentException(value);
        }
    }

<% end -%>
