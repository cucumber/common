    "babel-preset-env": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-config-eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.8.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
